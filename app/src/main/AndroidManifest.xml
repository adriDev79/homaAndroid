<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_homa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_homa_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homa">
        <activity android:name=".ihm.launchPage.launchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ihm.activity.ModifierSoldeActivity" />
        <activity android:name=".ihm.activity.AjouterSoldeActivity" />
        <activity android:name=".ihm.activity.ModifierDepenseAnnexeActivity" />
        <activity android:name=".ihm.activity.AjouterDepenseAnnexeActivity" />
        <activity android:name=".ihm.activity.ModifierDepenseFixeActivity" />
        <activity android:name=".ihm.activity.AjouterDepenseFixeActivity" />
        <activity android:name=".ihm.activity.ModifierRevenuActivity" />
        <activity android:name=".ihm.activity.AjouterRevenuActivity" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>